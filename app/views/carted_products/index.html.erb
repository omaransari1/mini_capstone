<h1>Checkout</h1>
<%= @carted_products.each do |carted_product| %>
  <h2>Name: <%=carted_product.product.name %></h2>
  <h2>Quantity: <%=carted_product.quantity %></h2>
  <%= link_to "Remove from cart", "/carted_products/#{carted_product.id}", method: :destroy do %>
<%end%>
<%= form_tag "/orders" method: :post do %>
  <%= submit_tag "Buy now" %>
<%end%>